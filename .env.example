# ============================================
# CRM Environment Configuration
# ============================================
# Copy this file to .env and fill in your values

# --------------------------------------------
# Supabase Configuration
# --------------------------------------------
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# --------------------------------------------
# N8N Webhook Configuration
# --------------------------------------------
# Single webhook URL that handles all upload types (routes by upload_type parameter)
# Get from your n8n workflow webhook node
VITE_N8N_WEBHOOK_URL=your_n8n_webhook_url_here

# --------------------------------------------
# Application Settings
# --------------------------------------------
# Set to 'false' for production, 'true' for testing with mock data
VITE_USE_MOCK_DATA=false

# Set to 'false' for now (authentication not implemented)
VITE_REQUIRE_AUTH=false